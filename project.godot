; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Chain Reaction"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Shaker="*res://addons/shaker/src/Shaker.gd"

[display]

window/size/viewport_width=1152.0
window/size/viewport_height=648.0
window/size/window_width_override=640.0
window/size/window_height_override=360.0
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/shaker/plugin.cfg")

[global_group]

Cell=""

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="Enemies"
2d_physics/layer_3="Hazards"
2d_physics/layer_4="Environment"
2d_physics/layer_5="Coins"

[rendering]

textures/canvas_textures/default_texture_filter="Nearest"
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
